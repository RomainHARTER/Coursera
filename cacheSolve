# Try to see if I've cached solve(matrix), if not, calculates it, else, returns it 
cacheSolve = function(x, ...) {
        m = x$getCacheMatrix()
        if(!is.null(m)) {
                message("getting cached data")
        } else {
                data = x$get()
                m    = solve(data, ...)
                x$setCacheMatrix(m)
        }
        m
}
